<script type="text/javascript">
var c=document.getElementById("myCanvas");
var ctx=c.getContext("2d");
setTimeout(function level1(){
var imgOb1 = new Image();

      imgOb1.onload = function() {
        ctx.drawImage(imgOb1, 0, 0 , 1260 , 660);
      };
      imgOb1.src = './bg/level1.jpg';},0);
setTimeout(function level2(){
var imgOb2 = new Image();

      imgOb2.onload = function() {
        ctx.drawImage(imgOb2, 0, 0 , 1260 , 660);
      };
      imgOb2.src = './bg/level2.jpg';},4000);
setTimeout(function level2(){
var imgOb3 = new Image();

      imgOb3.onload = function() {
        ctx.drawImage(imgOb3, 0, 0 , 1260 , 660);
      };
      imgOb3.src = './bg/level3.jpg';},8000);
setTimeout(function level4(){
var imgOb4 = new Image();

      imgOb4.onload = function() {
        ctx.drawImage(imgOb4, 0, 0 , 1260 , 660);
      };
      imgOb4.src = './bg/level4.jpg';},12000);
setTimeout(function level5(){
var imgOb5 = new Image();

      imgOb5.onload = function() {
        ctx.drawImage(imgOb5, 0, 0 , 1260 , 660);
      };
      imgOb5.src = './bg/level5.jpg';},16000);


var mouseX = 0;
var mouseY = 0;
window.addEventListener('mousemove', mouseMoved, true);
function mouseMoved(e) {
 mouseX = e.pageX;
 mouseY = e.pageY;
}

window.addEventListener('click', clicked, true);
function clicked(e) {
 alert("hello");
}


var ismousedown = 0;
window.onmousedown = function(e) {
 ismousedown = 1;
}
window.onmouseup = function(e) {
 ismousedown = 0;
}
</script>
